{
  "name": "@econeura/backend",
  "version": "1.0.0",
  "description": "ECONEURA Backend - DDD + CQRS + Hexagonal",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json --skipLibCheck",
    "start": "node dist/index.js",
    "type-check": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "test:integration": "cross-env GEMINI_API_KEY=test-key DATABASE_URL=postgres://mock:5432/mock USE_MEMORY_STORE=true jest tests/integration",
    "lint": "eslint .",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@azure/keyvault-secrets": "^4.10.0",
    "@azure/storage-blob": "^12.29.1",
    "@types/express-rate-limit": "^6.0.2",
    "applicationinsights": "^3.12.0",
    "axios": "^1.7.7",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.71.1",
    "pg": "^8.16.3",
    "rate-limit-redis": "^4.2.3",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",

    "jest": "^29.7.0",
    "supertest": "^7.1.4",

    "jest": "^30.2.0",
    "supertest": "^6.3.4",

    "ts-jest": "^29.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3"
  }
}